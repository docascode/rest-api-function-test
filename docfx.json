{
  "build": {
    "content": [
      {
        "src":"docs-ref-autogen",
        "files": [
          "**.json",
          "**.yml",
          "toc.md"
          ],
        "exclude": [ ],
        "dest": "api"        
      },
      {
        "src":"docs-ref-index",
        "files": [
          "**/*.md"
          ],
        "exclude": [ ] ,
        "dest": "api/power-bi"      
      }
    ],

    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/rest/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "titleSuffix": "Power BI REST API",
      "author": "eligr",
      "ms.author": "eligr",
      "ms.topic": "reference",
      "ms.devlang": "rest-api",
      "ms.date": "03/14/2018"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "powerbi-rest-api",
    "markdownEngineName": "markdig"
  }
}
